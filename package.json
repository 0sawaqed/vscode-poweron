{
    "name": "poweron",
    "displayName": "PowerOn",
    "description": "Syntax highlighting for Symitar's PowerOn Language",   
    "version": "1.0.0",
    "publisher": "0sawaqed",   
    "author": 
    {
        "name": "Oday Sawaqed",
        "email": "github@osawaqed.com"
    },  
    "licenses": 
    [
        {
            "type": "MIT",
            "url": "https://github.com/0sawaqed/vscode-poweron/blob/master/LICENSE"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/0sawaqed/vscode-poweron"
    },
    "bugs": {
        "url": "https://github.com/0sawaqed/vscode-poweron/issues"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "poweron",
            "aliases": ["PowerOn", "po", "poweron"],
            "extensions": [".PO",".DEF", ".SET", ".PRO", ".SUB",".POWERON"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "poweron",
            "scopeName": "source.poweron",
            "path": "./syntaxes/po.tmLanguage.json"
        }]
    }
}
